{
  "name": "drill-void",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/drill-void/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/drill-void/test/jest-e2e.json",
    "dev-getaway": "nest start --watch getaway",
    "dev-ms-auth": "nest start --watch ms-auth",
    "dev-ms-planet": "nest start --watch ms-planet",
    "dev-ms-ship": "nest start --watch ms-ship",
    "dev-ws-gateway": "nest start  --watch ws-gateway",
    "dev-ms-game-data": "nest start --watch ms-game-data",
    "prisma:gen": "npx libs/prisma/prisma generate --schema=./libs/prisma/prisma/schema.prisma --watch",
    "prisma:migration": "npx libs/prisma/prisma migrate dev --schema ./libs/prisma/prisma/schema.prisma",
    "prisma:seed": "npx prisma db seed",
    "prisma:reset": "npx libs/prisma/prisma migrate reset --schema=./libs/prisma/prisma/schema.prisma",
    "dev:all": "concurrently -c \"cyan.bold,red.bold,green.bold,yellow.bold,magenta.bold,blue.bold,purple.bold,magenta.bold,yellow.bold,green.bold,red.bold,cyan.bold\" \"yarn:dev-*\"",
    "testing": "concurrently -c \"cyan,red,green,yellow,blue,magenta\" \"nest start --watch getaway\" \"nest start --watch ms-auth\" \"nest start --watch ms-achievement\"  \"nest start --watch ms-planet\" \"nest start --watch ms-ship\""
  },
  "dependencies": {
    "@nestjs/bull": "^11.0.4",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.9",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/microservices": "^11.1.9",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/platform-socket.io": "^11.1.9",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/websockets": "^11.1.9",
    "@prisma/client": "^6.5.0",
    "@telegram-apps/init-data-node": "^2.0.10",
    "@types/passport-jwt": "^4.0.1",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "argon2": "^0.44.0",
    "bull": "^4.16.5",
    "bullmq": "^5.63.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "concurrently": "9.1.2",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "ioredis": "^5.8.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.6.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "simplex-noise": "^4.0.3",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.5.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/contracts(|/.*)$": "<rootDir>/libs/contracts/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/prisma(|/.*)$": "<rootDir>/libs/prisma/src/$1",
      "^@app/redis(|/.*)$": "<rootDir>/libs/redis/src/$1"
    }
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "prisma": {
    "seed": "ts-node libs/prisma/prisma/seed.ts all"
  }
}
